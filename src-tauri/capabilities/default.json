{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": ["main"],
  "permissions": [
    "path:default",
    "event:default",
    "window:default",
    "webview:default",
    "app:default",
    "resources:default",
    "image:default",
    "menu:default",
    "tray:default",
    "shell:allow-open",
    "os:allow-arch",
    "os:allow-os-type",
    "window:allow-start-dragging",
    "window:allow-toggle-maximize",
    "window:allow-minimize",
    "window:allow-close"
  ]
}
